<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cu">

<select id="select" resultType='customer.CustomerVO'>
select * from customer order by 1
</select>

<delete id="delete">
delete customer where id = #{id}
</delete>
<insert id="insert">
	insert into and_member values (
	#{email},#{pw},#{name},#{gender},#{file_path,jdbcType=VARCHAR}
	)
</insert>
</mapper>